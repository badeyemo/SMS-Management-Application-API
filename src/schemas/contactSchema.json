{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "data"
  ],
  "properties": {
    "data": {
      "$id": "#/properties/data",
      "type": "object",
      "title": "The Data Schema",
      "required": [
        "name",
        "phone_number"
      ],
      "properties": {
        "name": {
          "$id": "#/properties/data/properties/name",
          "type": "string",
          "title": "The Name Schema",
          "default": "",
          "examples": [
            "Raphael Etim"
          ],
          "pattern": "^(.*)$"
        },
        "phone_number": {
          "$id": "#/properties/data/properties/phone_number",
          "type": "string",
          "title": "The Phone_number Schema",
          "default": "",
          "examples": [
            "07031036892"
          ],
          "pattern": "^(.*)$"
        }
      }
    }
  }
}